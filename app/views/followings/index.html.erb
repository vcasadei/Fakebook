<h1>Listing profiles</h1>

<table>
  <tr>
    <th>Full name</th>
    <th></th>
    <th></th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= profile.full_name %></td>
    <td><%= link_to 'View profile', profile %></td>
    <td><%= link_to 'Follow', followings_path(:id => profile), :method => :post %></td>
  </tr>
<% end %>
</table>

<br />